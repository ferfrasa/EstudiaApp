<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Nombre project</th>
      <th>Descripcion project</th>
      <th>Califiacion projeact</th>
      <th>Estado project</th>
      <th>Category</th>
      <th>Spectator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.nombre_project %></td>
        <td><%= project.descripcion_project %></td>
        <td><%= project.califiacion_projeact %></td>
        <td><%= project.estado_project %></td>
        <td><%= project.category %></td>
        <td><%= project.spectator %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
